	            android:id="@+id/file_save_external_public_list_btn"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="1"
	            android:text="@string/file_save_list"
	            android:onClick="forClick"
	            />
	        <Button 
	            android:id="@+id/file_save_external_public_save_btn"
